# .env.example (Production settings)

# API Key for production service
API_KEY=your-production-api-key

# OpenSearch configuration for production
OPENSEARCH_HOST=https://your-production-opensearch-host
OPENSEARCH_USER=your-opensearch-username
OPENSEARCH_PWD=your-opensearch-password

# Anthropic production API key
ANTHROPIC_PRODUCTION_API_KEY=your-anthropic-production-api-key

# OpenAI production API key
PRODUCTION_OPENAI_API_KEY=your-openai-production-api-key
